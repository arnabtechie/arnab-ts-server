{
  "name": "arnab-ts-server",
  "version": "1.0.0",
  "description": "API Server Build on Node, Express, TypeScript & MongoDB",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build_start": "npm run build && node server.js",
    "build": "tsc --project tsconfig.json",
    "dev": "nodemon server.js",
    "ts_dev": "tsc --watch && npm run ts_start",
    "watch": "tsc --watch",
    "ts_start": "ts-node server.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arnabtechie/arnab-ts-server.git"
  },
  "author": "Arnab Banerjee",
